# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true

[*]
indent_style             = space
trim_trailing_whitespace = true
insert_final_newline     = false
indent_size = 2
charset = utf-8
end_of_line = lf

[*.md]
trim_trailing_whitespace = false

[*.bat]
end_of_line = crlf

